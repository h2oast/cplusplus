#include <cstdio>
#include "gtest/gtest.h"

////////////////////////////////////////////////////////////////////////////////
//
//                               LEARN TEST ENTRY
// 
////////////////////////////////////////////////////////////////////////////////
//
// 

GTEST_API_ int main(int argc, char **argv) {
    int         rc;
    printf("<<<<<<<<<<<< Running main() from %s\n", __FILE__);


    //////////////////////////////////////////////////////////
    //
    //  googletest infrastructure setup
    // 
    ////////////////////////////////////////////////////////// 
    //
    // 
    testing::InitGoogleTest(&argc, argv);

    //////////////////////////////////////////////////////////
    //
    //  RUN our testcases for learn
    // 
    ////////////////////////////////////////////////////////// 
    //
    // 
    rc = RUN_ALL_TESTS();
    return rc;
}
#endif
